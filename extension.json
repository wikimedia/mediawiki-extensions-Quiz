{
	"name": "Quiz",
	"version": "1.2.0",
	"author": "Louis-RÃ©mi Babe",
	"url": "https://www.mediawiki.org/wiki/Extension:Quiz",
	"descriptionmsg": "quiz_desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.41"
	},
	"MessagesDirs": {
		"QuizExtension": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Quiz\\": "includes/"
	},
	"ResourceModules": {
		"ext.quiz": {
			"scripts": "ext.quiz.js",
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.quiz.styles": {
			"styles": "ext.quiz.css",
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Quiz/modules"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"MediaWiki\\Extension\\Quiz\\Hooks::onParserFirstCallInit"
		],
		"ParserAfterTidy": [
			"MediaWiki\\Extension\\Quiz\\Quiz::resetQuizID"
		]
	},
	"manifest_version": 2
}
